	// %f : 실수형
	// %c : 문자
	// %s : 문자열


	printf("number");
	printf("결과값은 %d 입니다", 30 - 20);

	printf("number");
	printf("결과값은 %d 입니다", 30 * 20);

	printf("number");
	printf("결과값은 %d 입니다", 30 / 20);

	printf("number");
	printf("결과값은 %d 입니다", 30 % 20);

	printf("number");
